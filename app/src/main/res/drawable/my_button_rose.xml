<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#7f863ccc" />
            <gradient android:angle="-90" android:startColor="#7f863ccc" android:endColor="#6d621ec2"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#7f863ccc" />
            <solid android:color="#7f863ccc"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#7f863ccc" />
            <gradient android:angle="-90" android:startColor="#6d621ec2" android:endColor="#6d621ec2" />
        </shape>
    </item>
</selector>